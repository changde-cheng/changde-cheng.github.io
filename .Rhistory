chisq.test(x=c(25,95),y=c(1,166))
# Study parameters
total_patients <- 3663
total_glycine_sites <- 168
g972_mutations <- 25
other_glycine_mutations <- 95
other_glycine_sites <- total_glycine_sites - 1  # 167 other sites
# For G972 (1 site)
g972_mutated <- g972_mutations
g972_not_mutated <- total_patients - g972_mutations
# For other glycine sites (167 sites)
other_mutated <- other_glycine_mutations
other_not_mutated <- (total_patients * other_glycine_sites) - other_glycine_mutations
# Create contingency table
contingency_table <- matrix(c(g972_mutated, g972_not_mutated,
other_mutated, other_not_mutated),
nrow = 2, byrow = TRUE,
dimnames = list(c("G972", "Other_Glycines"),
c("Mutated", "Not_Mutated")))
print("Contingency Table:")
print(contingency_table)
# Chi-square test of independence
chisq_indep <- chisq.test(contingency_table)
print(chisq_indep)
cat("\nOdds Ratio:")
odds_ratio <- (contingency_table[1,1] * contingency_table[2,2]) /
(contingency_table[1,2] * contingency_table[2,1])
cat(round(odds_ratio, 2), "\n")
# Total glycine mutations
total_glycine_mutations <- g972_mutations + other_glycine_mutations
# Expected probability for G972 (1 site out of 168)
expected_prob <- 1 / total_glycine_sites
# Binomial test: Is G972 getting more than its "fair share" of mutations?
binom_test <- binom.test(x = g972_mutations,
n = total_glycine_mutations,
p = expected_prob,
alternative = "greater")
print(binom_test)
